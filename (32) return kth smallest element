int kthSmallest(int arr[], int l, int r, int k) {
        //code here
        int n=r-l+1;
        int ans;
        sort(arr,arr+n);
        for(int i=0;i<n;i++){
            if(i==k-1){
                return arr[i];
            }
        }
    
