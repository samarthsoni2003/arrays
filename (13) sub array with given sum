https://practice.geeksforgeeks.org/problems/subarray-with-given-sum-1587115621/1?page=1&difficulty[]=0&category[]=Arrays&sortBy=submissions

#include <bits/stdc++.h>
using namespace std;
class Solution
{
    public:
    vector<int> subarraySum(vector<int>arr, int n, long long s)
    {
        int last=0;
        int start=0;
        long long  currsum=0;
        bool flag=false;
        vector<int>res;
        for(int i=0;i<n;i++)
        {
            currsum += arr[i];
            if(currsum>=s)
            {
                last=i;
                while(s<currsum && start<last){
                currsum -= arr[start];
                ++start;
                }
                if(currsum==s)
                {
                    res.push_back(start + 1);
                    res.push_back(last + 1);
                    flag = true;
                    break;
                }
            }
        }
        if(flag==false)
            res.push_back(-1);
        return res;    
    }
};
